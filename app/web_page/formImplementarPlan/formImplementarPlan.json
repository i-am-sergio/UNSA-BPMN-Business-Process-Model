      "type" : "expression",
      "value" : [
        "if( $data.operacionEmergencia ){",
        "\treturn {",
        "\t\t//map operacionEmergencia variable to expected task contract input",
        "\t\toperacionEmergenciaInput: {",
        "\t\t\tdescripcion: $data.operacionEmergencia.descripcion !== undefined ? $data.operacionEmergencia.descripcion : null,",
