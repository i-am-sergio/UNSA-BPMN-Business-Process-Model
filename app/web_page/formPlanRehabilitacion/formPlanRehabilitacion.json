    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.planReconstruccion ){",
        "\treturn {",
        "\t\t//map planReconstruccion variable to expected task contract input",
        "\t\tplanReconstruccionInput: {",
        "\t\t\tfechaInicio: $data.planReconstruccion.fechaInicio !== undefined ? $data.planReconstruccion.fechaInicio : null,",
        "\t\t\tfechaFin: $data.planReconstruccion.fechaFin !== undefined ? $data.planReconstruccion.fechaFin : null,",
