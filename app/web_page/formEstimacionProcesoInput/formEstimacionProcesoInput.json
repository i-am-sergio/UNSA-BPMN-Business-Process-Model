    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.procesoEstimacion ){",
        "\treturn {",
        "\t\t//map procesoEstimacion variable to expected task contract input",
        "\t\tprocesoEstimacionInput: {",
        "\t\t\tnombreProceso: $data.procesoEstimacion.nombreProceso !== undefined ? $data.procesoEstimacion.nombreProceso : null,",
        "\t\t\tdescripcion: $data.procesoEstimacion.descripcion !== undefined ? $data.procesoEstimacion.descripcion : null,",
        "\t\t\tfechaInicio: $data.procesoEstimacion.fechaInicio !== undefined ? $data.procesoEstimacion.fechaInicio : null,",
        "\t\t\tfechaFin: $data.procesoEstimacion.fechaFin !== undefined ? $data.procesoEstimacion.fechaFin : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
