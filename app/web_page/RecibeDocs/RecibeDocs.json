    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.cargaDocs ){",
        "\treturn {",
        "\t\t//map cargaDocs variable to expected task contract input",
        "\t\tcargaDocsInput: {",
        "\t\t\tdiagnosticoRSU: $data.cargaDocs.diagnosticoRSU !== undefined ? $data.cargaDocs.diagnosticoRSU : null,",
        "\t\t\treporteAnualRSU: $data.cargaDocs.reporteAnualRSU !== undefined ? $data.cargaDocs.reporteAnualRSU : null,",
        "\t\t\tpei: $data.cargaDocs.pei !== undefined ? $data.cargaDocs.pei : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
