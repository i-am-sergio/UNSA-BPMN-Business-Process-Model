    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.pamc ){",
        "\treturn {",
        "\t\t//map pamc variable to expected task contract input",
        "\t\tpamcInput: {",
        "\t\t\tid: $data.pamc.id !== undefined ? $data.pamc.id : null,",
        "\t\t\tdescripcion: $data.pamc.descripcion !== undefined ? $data.pamc.descripcion : null,",
