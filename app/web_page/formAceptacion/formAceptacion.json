    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.planGestionRiesgos ){",
        "\treturn {",
        "\t\t//map planGestionRiesgos variable to expected task contract input",
        "\t\tplanGestionRiesgosInput: {",
        "\t\t\tnombrePlan: $data.planGestionRiesgos.nombrePlan !== undefined ? $data.planGestionRiesgos.nombrePlan : null,",
        "\t\t\tdescripcion: $data.planGestionRiesgos.descripcion !== undefined ? $data.planGestionRiesgos.descripcion : null,",
        "\t\t\tesAdecuado: $data.planGestionRiesgos.esAdecuado !== undefined ? $data.planGestionRiesgos.esAdecuado : null,",
        "\t\t\tfechaElaboracion: $data.planGestionRiesgos.fechaElaboracion !== undefined ? $data.planGestionRiesgos.fechaElaboracion : null,",
        "\t\t\tfechaModificacion: $data.planGestionRiesgos.fechaModificacion !== undefined ? $data.planGestionRiesgos.fechaModificacion : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
