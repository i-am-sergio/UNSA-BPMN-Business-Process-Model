      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.implementacionAccionesPamc ){",
        "\treturn {",
        "\t\t//map implementacionAccionesPamc variable to expected task contract input",
        "\t\timplementacionAccionesPamcInput: {",
        "\t\t\tfechaInicio: $data.implementacionAccionesPamc.fechaInicio !== undefined ? $data.implementacionAccionesPamc.fechaInicio : null,",
