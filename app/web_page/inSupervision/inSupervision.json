    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.evaluacionDelPamc ){",
        "\treturn {",
        "\t\t//map evaluacionDelPamc variable to expected task contract input",
        "\t\tevaluacionDelPamcInput: {",
        "\t\t\tfechaDeRevision: $data.evaluacionDelPamc.fechaDeRevision !== undefined ? $data.evaluacionDelPamc.fechaDeRevision : null,",
        "\t\t\tcomentarios: $data.evaluacionDelPamc.comentarios !== undefined ? $data.evaluacionDelPamc.comentarios : null,",
        "\t\t\taprobacion: $data.evaluacionDelPamc.aprobacion !== undefined ? $data.evaluacionDelPamc.aprobacion : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
