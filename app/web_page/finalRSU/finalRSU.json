    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.entradaDesarrollo && $data.informeFinal && $data.procesoPlanificar ){",
        "\treturn {",
        "\t\t//map entradaDesarrollo variable to expected task contract input",
        "\t\tentradaDesarrolloInput: {",
        "\t\t\trsu: $data.entradaDesarrollo.rsu !== undefined ? $data.entradaDesarrollo.rsu : null",
        "\t\t},",
        "\t\t//map informeFinal variable to expected task contract input",
        "\t\tinformeFinalInput: {",
        "\t\t\trsufinal: $data.informeFinal.rsufinal !== undefined ? $data.informeFinal.rsufinal : null",
        "\t\t},",
        "\t\t//map procesoPlanificar variable to expected task contract input",
        "\t\tprocesoPlanificarInput: {",
        "\t\t\tplanificacion: $data.procesoPlanificar.planificacion !== undefined ? $data.procesoPlanificar.planificacion : null,",
        "\t\t\taprobar: $data.procesoPlanificar.aprobar !== undefined ? $data.procesoPlanificar.aprobar : null",
        "\t\t}",
        "\t}",
        "}"
      ],
