      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.recoleccionDatos ){",
        "\treturn {",
        "\t\t//map recoleccionDatos variable to expected task contract input",
        "\t\trecoleccionDatosInput: {",
        "\t\t\tfechaRecoleccion: $data.recoleccionDatos.fechaRecoleccion !== undefined ? $data.recoleccionDatos.fechaRecoleccion : null,",
        "\t\t\tmetodoRecoleccion: $data.recoleccionDatos.metodoRecoleccion !== undefined ? $data.recoleccionDatos.metodoRecoleccion : null,",
