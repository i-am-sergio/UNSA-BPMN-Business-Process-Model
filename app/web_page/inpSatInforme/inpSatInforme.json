    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.informeDatos ){",
        "\treturn {",
        "\t\t//map informeDatos variable to expected task contract input",
        "\t\tinformeDatosInput: {",
        "\t\t\tfechaInforme: $data.informeDatos.fechaInforme !== undefined ? $data.informeDatos.fechaInforme : null,",
