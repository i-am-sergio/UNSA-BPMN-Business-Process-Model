      "value" : [
        "if( $data.aprobar_estrategia ){",
        "\treturn {",
        "\t\t//map aprobar_estrategia variable to expected task contract input",
        "\t\taprobar_estrategiaInput: {",
        "\t\t\tfecha: $data.aprobar_estrategia.fecha !== undefined ? $data.aprobar_estrategia.fecha : null,",
