      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.pamc ){",
        "\treturn {",
        "\t\t//map pamc variable to expected task contract input",
        "\t\tpamcInput: {",
        "\t\t\tdescripcion: $data.pamc.descripcion !== undefined ? $data.pamc.descripcion : null,",
        "\t\t\tfechaInicio: $data.pamc.fechaInicio !== undefined ? $data.pamc.fechaInicio : null,",
        "\t\t\tfechaFin: $data.pamc.fechaFin !== undefined ? $data.pamc.fechaFin : null,",
        "\t\t\testado: $data.pamc.estado !== undefined ? $data.pamc.estado : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
