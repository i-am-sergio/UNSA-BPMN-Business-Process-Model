    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.cursosss ){",
        "\treturn {",
        "\t\t//map cursosss variable to expected task contract input",
        "\t\tcursosssInput: {",
        "\t\t\tfullName: $data.cursosss.fullName !== undefined ? $data.cursosss.fullName : null,",
        "\t\t\tacademicPerformance: $data.cursosss.academicPerformance !== undefined ? $data.cursosss.academicPerformance : null,",
        "\t\t\tcredit: $data.cursosss.credit !== undefined ? $data.cursosss.credit : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
