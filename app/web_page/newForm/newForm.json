        "\t\tpeiDirectorOficinaInput: {",
        "\t\t\tnecesidadesPresupuestarias: $data.peiDirectorOficina.necesidadesPresupuestarias !== undefined ? $data.peiDirectorOficina.necesidadesPresupuestarias : null",
