      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.actaDeDepartamentos ){",
        "\treturn {",
        "\t\t//map actaDeDepartamentos variable to expected task contract input",
        "\t\tactaDeDepartamentosInput: {",
        "\t\t\tdepartamentosInvolucrados: $data.actaDeDepartamentos.departamentosInvolucrados !== undefined ? $data.actaDeDepartamentos.departamentosInvolucrados : null,",
        "\t\t\tfechaDeCoordinacion: $data.actaDeDepartamentos.fechaDeCoordinacion !== undefined ? $data.actaDeDepartamentos.fechaDeCoordinacion : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
