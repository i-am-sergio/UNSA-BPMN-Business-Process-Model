    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.recibir_informacion && $data.elaborar_rsu && $data.aprobar_rsu ){",
        "\treturn {",
        "\t\t//map recibir_informacion variable to expected task contract input",
        "\t\trecibir_informacionInput: {",
        "\t\t\tdiagnostico: $data.recibir_informacion.diagnostico !== undefined ? $data.recibir_informacion.diagnostico : null,",
        "\t\t\tinforme: $data.recibir_informacion.informe !== undefined ? $data.recibir_informacion.informe : null,",
        "\t\t\tpei: $data.recibir_informacion.pei !== undefined ? $data.recibir_informacion.pei : null",
        "\t\t},",
        "\t\t//map elaborar_rsu variable to expected task contract input",
        "\t\telaborar_rsuInput: {",
        "\t\t\trsu: $data.elaborar_rsu.rsu !== undefined ? $data.elaborar_rsu.rsu : null",
        "\t\t},",
        "\t\t//map aprobar_rsu variable to expected task contract input",
        "\t\taprobar_rsuInput: {",
        "\t\t\taprobado: $data.aprobar_rsu.aprobado !== undefined ? $data.aprobar_rsu.aprobado : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
